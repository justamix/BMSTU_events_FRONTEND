{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "FRONTEND",
  "version": "0.1.0",
  "identifier": "com.mgtu.frontend", 
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://192.168.68.107:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "FRONTEND",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
    ]
}
}
